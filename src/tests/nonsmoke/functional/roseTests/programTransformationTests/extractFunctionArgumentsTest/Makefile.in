	rm -f $(normalizationTranslator_TEST_TARGETS) $(normalizationTranslator_TEST_TARGETS:.passed=.failed)
	rm -f $(broken_TEST_TARGETS) $(broken_TEST_TARGETS:.passed=.failed)
	rm -f -- *.C

