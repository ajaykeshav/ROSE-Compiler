    if test "$pqxx" = yes; then
        ROSE_LIBPQXX_PREFIX=
        LIBS="$LIBS -lpqxx -lpq"
    elif test -n "$pqxx" -a "$pqxx" != no; then
        ROSE_LIBPQXX_PREFIX="$pqxx"
