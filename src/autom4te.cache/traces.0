    if test $pqxx = no; then
        AC_MSG_NOTICE([skipping check for libpqxx])
    else
        AC_CHECKING([for libpqxx])
        AC_LANG_PUSH([C++])
        AC_LINK_IFELSE([
            AC_LANG_SOURCE([
